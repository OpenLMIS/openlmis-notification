{
  "service": {
    "Name": "notification",
    "Id": "notification-service",
    "Port": 8080,
    "Tags": ["openlmis-service"],
    "check": {
      "interval": "10s",
      "http": "http://HOST:PORT/actuator/health",
      "timeout": "5s",
      "deregisterCriticalServiceAfter": "10m"
    }
  },
  "raml": "/consul/api-definition.yaml",
  "path": [
    "notification",
    "notification/docs",
    "notification/docs/<all>",
    "notification/webjars",
    "notification/webjars/<all>"
  ]
}
